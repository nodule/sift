{
  "name": "sift",
  "description": "Sift for ChiÏ‡",
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodule/sift.git"
  },
  "dependencies": {
    "sift": "^0.2.3"
  },
  "nodes": [
    {
      "ports": {
        "input": {
          "filter": {
            "title": "Filter",
            "type": "any"
          },
          "in": {
            "title": "Document",
            "type": "object",
            "async": true
          }
        },
        "output": {
          "out": {
            "title": "New Document",
            "type": "object"
          },
          "error": {
            "title": "Error",
            "type": "Error"
          }
        }
      },
      "dependencies": {
        "npm": {
          "sift": "latest"
        }
      },
      "phrases": {
        "active": "Filtering array"
      },
      "async": true,
      "description": "MongoDB inspired array filtering",
      "ns": "sift",
      "name": "filter",
      "fn": "on.input.in = function() {\n  try {\n    output({out: $.create(sift($.filter, $.in))});\n  } catch (e) {\n    output({error: $.create(e)});\n  }\n};\n"
    }
  ],
  "twigs": []
}